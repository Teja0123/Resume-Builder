{
  "routes": [
    { "src": "/?", "dest": "/loginpage.html" },
    { "src": "/loginpage.html", "dest": "/loginpage.html" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js|html)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ],
  "cleanUrls": true
}
